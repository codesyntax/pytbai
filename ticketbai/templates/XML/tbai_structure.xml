<?xml version="1.0" encoding="UTF-8"?>
<T:TicketBai xmlns:T="urn:ticketbai:emision" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:ticketbai:emision ticketBaiV1-2-1.xsd ">
   <Cabecera>
      <IDVersionTBAI>${version}</IDVersionTBAI>
   </Cabecera>
   <Sujetos>
      <Emisor>
         <NIF>${entity_id}</NIF>
         <ApellidosNombreRazonSocial>${name}</ApellidosNombreRazonSocial>
      </Emisor>
      <VariosDestinatarios>${multi_recipient}</VariosDestinatarios>
      <EmitidaPorTercerosODestinatario>${external_invoice}</EmitidaPorTercerosODestinatario>
   </Sujetos>
   <Factura>
      <CabeceraFactura>
         <SerieFactura>${serial_code}</SerieFactura>
         <NumFactura>${num}</NumFactura>
         <FechaExpedicionFactura>${expedition_date}</FechaExpedicionFactura>
         <HoraExpedicionFactura>${expedition_time}</HoraExpedicionFactura>
         <FacturaSimplificada>${simplified}</FacturaSimplificada>
         <FacturaEmitidaSustitucionSimplificada>${substitution}</FacturaEmitidaSustitucionSimplificada>
      </CabeceraFactura>
      <DatosFactura>
         <FechaOperacion>${transaction_date}</FechaOperacion>
         <DescripcionFactura>${description}</DescripcionFactura>
         <DetallesFactura></DetallesFactura>
         <ImporteTotalFactura></ImporteTotalFactura>
         <Claves>
            <IDClave>
               <ClaveRegimenIvaOpTrascendencia>${vat_regime}</ClaveRegimenIvaOpTrascendencia>
            </IDClave>
         </Claves>
      </DatosFactura>
      <TipoDesglose>
         <DesgloseFactura>
            <Sujeta>
               <NoExenta></NoExenta>
            </Sujeta>
         </DesgloseFactura>
      </TipoDesglose>
   </Factura>
   <HuellaTBAI>
      <Software>
         <LicenciaTBAI>${license}</LicenciaTBAI>
         <EntidadDesarrolladora>
            <NIF>${dev_entity}</NIF>
         </EntidadDesarrolladora>
         <Nombre>${soft_name}</Nombre>
         <Version>${soft_version}</Version>
      </Software>
   </HuellaTBAI>
</T:TicketBai>
